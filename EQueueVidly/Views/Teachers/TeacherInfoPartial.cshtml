@using Microsoft.AspNet.Identity
@model EQueueVidly.ViewModels.IndexViewModel


<div class="panel panel-success">
    <div class="panel-heading" style="position: relative;">
        <h3 class="panel-title">@Model.FirstName @Model.LastName</h3>
    </div>
    <div class="panel-body">
        <div class="row" style="position: relative;">
            <img style="position: relative; bottom: 20px;" src="@Url.Action("Photo", "Account" , new { UserId=User.Identity.GetUserId() })" height="400" width="400" alt=" " class=" img-thumbnail img-responsive" />
        </div>

        <div class="row" style="padding: 10px;">
            <p style="margin-bottom: 5px"> <span style="padding-left: 7px; padding-right: 7px"><i class="fa fa-envelope fa" aria-hidden="true"></i></span>@Model.Email</p>
            <p style="margin-bottom: 5px"> <span style="padding-left: 7px; padding-right: 7px"><i class="fa fa-phone fa" aria-hidden="true"></i></span>@Model.Phone</p>
            
        </div>

    </div>
</div>

 

