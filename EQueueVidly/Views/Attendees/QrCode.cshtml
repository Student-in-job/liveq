@model EQueueVidly.ViewModels.QrCodeViewModel
@using EQueueVidly.Helpers

@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_CleanPage.cshtml";
}
<div class="row">
    <div class="panel panel-success main-center" style="max-width: 360px;">
        <div class="panel-heading">
            <h2 class="panel-title">QR code</h2>       
        </div>
        <div class="panel-body">
            @Html.GenerateQrCode("Student name: " + Model.StudentName + " | Start: " + Model.Start + " | End: " + Model.End)
        </div>
    </div>

</div>

